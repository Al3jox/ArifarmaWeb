@model IEnumerable<Arifarma.Models.Factura>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CostoTotal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodClienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodEmpleadoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CostoTotal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodClienteNavigation.CodCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodEmpleadoNavigation.CodEmpleado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodFactura">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodFactura">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodFactura">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
